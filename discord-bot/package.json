{
  "name": "discord-bot",
  "version": "1.0.0",
  "description": "A TypeScript Discord bot with AI integration using discordx",
  "license": "ISC",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "dev:clean": "npm run kill-dev && tsx watch src/index.ts",
    "deploy": "tsx src/deploy-commands.ts",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "biome check . && prettier --check .",
    "clean": "del-cli dist",
    "typecheck": "tsc --noEmit",
    "duplication": "jscpd src/",
    "duplication:report": "jscpd src/ --reporters html,console",
    "quality": "npm run lint && npm run typecheck && npm run duplication",
    "test": "npm run test:master",
    "test:master": "tsx scripts/master-test.ts",
    "test:unit": "npm run test:master -- --unit",
    "test:integration": "npm run test:master -- --integration",
    "test:docker": "npm run test:master -- --docker",
    "test:security": "tsx tests/integration/security.test.ts",
    "test:docker:health": "tsx -e \"import('./tests/utils/docker-health.js').then(m => m.printHealthSummary())\"",
    "test:all": "npm run test:master -- --all",
    "test:full": "npm run test:master -- --all",
    "test:fuzz": "vitest run tests/fuzz/",
    "kill-dev": "node -e \"const {execSync}=require('child_process');try{if(process.platform==='win32'){execSync('powershell -ExecutionPolicy Bypass -File scripts/kill-dev.ps1',{stdio:'inherit'})}else{execSync('bash scripts/kill-dev.sh',{stdio:'inherit'})}}catch{}\"",
    "docker:up": "npm run kill-dev && docker compose up -d",
    "docker:rebuild": "npm run kill-dev && docker compose up -d --build discord-bot",
    "prepare": "node -e \"console.log('Run: pip install pre-commit && pre-commit install')\" || exit 0"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript",
    "discordx",
    "ai"
  ],
  "author": "",
  "type": "module",
  "dependencies": {
    "@chroma-core/ollama": "^0.1.8",
    "@discordx/importer": "^1.3.3",
    "@discordx/pagination": "^4.4.0",
    "@discordx/utilities": "^8.0.0",
    "@modelcontextprotocol/sdk": "^1.24.2",
    "axios": "^1.13.2",
    "bullmq": "^5.65.1",
    "chromadb": "^3.1.6",
    "chrono-node": "^2.9.0",
    "discord.js": "^14.25.1",
    "discordx": "^11.13.2",
    "dompurify": "^3.3.0",
    "dotenv": "^17.2.3",
    "fast-xml-parser": "^5.3.2",
    "glob": "^13.0.0",
    "iovalkey": "^0.3.1",
    "jsdom": "^27.2.0",
    "lru-cache": "^11.2.4",
    "mathjs": "^15.1.0",
    "reflect-metadata": "^0.2.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@cloudflare/workers-types": "^4.20251205.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.1",
    "@types/prompts": "^2.4.9",
    "chalk": "^5.6.2",
    "del-cli": "^7.0.0",
    "fast-check": "^4.4.0",
    "jscpd": "^4.0.5",
    "prettier": "^3.7.4",
    "prompts": "^2.4.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
